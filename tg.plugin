#!name=Telegram 
#!desc=支持 Turrit/Swiftgram 强制跳转。
#!author=Roger
#!icon=bolt.fill
#!date=2025-12-30

[Argument]
跳转客户端 = select, "Turrit", "Swiftgram", "iMe", "Nicegram", "Telegram"

[Script]
# 指向刚才新建的 tg.js
http-request ^https?:\/\/(www\.)?(t|telegram)\.(me|dog)\/ script-path=https://cdn.jsdelivr.net/gh/RyderHee/LOON@main/tg_final.js, timeout=10, tag=TG重定向, argument="{跳转客户端}"

[MitM]
hostname = t.me, telegram.me, telegram.dog
