#!name=TG Universal
#!desc=多客户端跳转
#!author=Roger
#!date=2025-12-30

[Argument]
App = select, "Turrit", "Swiftgram", "iMe", "Nicegram", "Telegram"

[Rule]
DOMAIN-SUFFIX, t.me, PROXY

[MitM]
hostname = t.me

[Script]
http-request ^https?:\/\/(www\.)?t\.me\/ script-path=https://raw.githubusercontent.com/RyderHee/LOON/main/tg.js, timeout=10, tag=TGJump, argument="{App}"
