#!name=Telegram 多客户端跳转
#!desc=支持选择第三方客户端跳转 (Turrit, Swiftgram等)。
#!author=Roger
#!icon=paperplane.fill
#!select=跳转客户端, Telegram, Swiftgram, Turrit, iMe, Nicegram

[Argument]
跳转客户端 = select, "Telegram", "Swiftgram", "Turrit", "iMe", "Nicegram", "Liao"

[Script]
http-request ^https?:\/\/(www\.)?(t|telegram)\.(me|dog)\/ script-path=https://cdn.jsdelivr.net/gh/RyderHee/LOON@main/tg.js, timeout=10, tag=Telegram跳转

[MitM]
hostname = t.me, telegram.me, telegram.dog
